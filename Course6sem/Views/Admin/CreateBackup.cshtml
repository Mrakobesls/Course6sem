@using Application.Models.Admin

@model CreateBackupRequest

<h2>Создание уровня доступа</h2>

@using (Html.BeginForm("CreateBackup", "Admin", FormMethod.Post))
{
    <div class="validation" asp-validation-summary="ModelOnly"></div>
    <div>
        <div class="form-group">
            @Html.ValidationMessageFor(m => m.Name)
            @Html.LabelFor(m => m.Name, "Название")
            @Html.EditorFor(m => m.Name)
        </div>
        <div class="form-group">
            <input type="submit" value="Создать" class="btn btn-outline-dark" />
        </div>
    </div>
}
